																						<sixx.object sixx.id="1287" sixx.name="string" sixx.type="String" sixx.env="Core" >   if &#40;NumIRQ==99&#41; L=500;      //--если номер прерывания =99 - пин ECHO не обслуживает прерывание      
    else {  
         //---Получаем время импульса
        if &#40;time_ANSWER &#33;=  timeANSWER_old&#41; {
            timeANSWER_old =  time_ANSWER ;
            L= time_ANSWER;
            L&#42;=sqrt&#40;273+t&#41;;           //---Умножаем время на корень из температуры в кельвинах
             L /=1000;                    //---Делим полученное число на 1000
            if &#40; L &gt;400&#41;  L =400;      //---Если результат больше 400 см &#40;4 м&#41; значит это ошибка
            }
          }   
      return L;
