											<sixx.object sixx.id="1507" sixx.name="string" sixx.type="String" sixx.env="Core" >//-----Инициализация - выполняется один раз и только если взведен вход разрешения EN
if &#40; &#33; INIT &amp;&amp; EN&#41; 
{     InitSR04&#40; ECHO&#44; TRIG &#41;;     //----Инициализация портов ECHO и TRIG и определение номера 
                                              //       вектора прерывания NumIRQ=&#40;0&#44;1&#44;2&#44;3&#44;4&#44;5&#41;.
                                              //   Если пин ECHO не соответствует пину&#44; который способен обрабатывать прерывания&#44;
                                              //  NumIRQ=99;
      WDTime=StartTime = millis&#40;&#41;;   //----Устанавливаем начальное время отсчета и сторожевой таймер
       INIT=true;                         //----Взводим флаг &quot;ИНИЦИАЛИЗАЦИЯ&quot; 
       Led=false;                       //-----Сбрасываем индикатор измерений 
       Lcm =2;                          //----Устанавливаем предварительно дистанцию - пусть это будет 2 см
}
//================================================
//-------Если нет разрешения на опрос - выход c кодом ошибки 401
if &#40; &#33;EN &#41;                   { Lcm =401; Led=false; goto End1;}   
//------Если ECHO настроен на пин без прерываний - выход с кодом ошибки 402 
if &#40; NumIRQ ==99&#41;    { Lcm =402; Led=false; goto End1;}    
// -------Измерение  
  if &#40;T_ms==0&#41;  {  StartSR04&#40;&#41;; }
   else {  if &#40;_isTimer&#40;StartTime&#44;  T_ms &#41;&#41;  { StartSR04&#40;&#41;;  StartTime = millis&#40;&#41;; } }
//------------------------------------------------------------------------------------------------------------
//-----Если тайм-аут&#44; то возможно завис датчик. 
//     Перезапускаем его как рекомендовали http://forum.arduino.cc/index.php?topic=55119.15
//       &#40;замыкаем выход ECHO на землю в режиме вывод&#41;.
//-----------------------------------------------------------------------------------------------------------
if   &#40; flag_REQUEST &amp;&amp; &#40;_isTimer &#40;WDTime&#44;  500 &#41;&#41;&#41;
  {    
     WDTime=millis&#40;&#41;; 
       Lcm =403;                  //----Устанавливаем  дистацию с кодом ошибки 403
       Led=false;                   //-----Сбрасываем индикатор измерений       
       switch &#40;StageReset&#41;     //-----Выбираем этап обработчика алгоритма аппаратного сброса датчика
        {
         case 0: StageReset++; break;                                          //---Просто пауза длиной уставки тайм-аута   
         case 1: pinMode&#40; ECHO&#44; OUTPUT&#41;; StageReset++; break; //---пин ECHO настраивается как выход и пауза
         case 2: digitalWrite&#40; ECHO&#44; LOW&#41;;  StageReset++; break; //---пин ECHO на низкий уровень и пауза
         case 3: StageReset++; break;                                          //---Снова пауза   
         case 4: pinMode&#40; ECHO&#44; INPUT&#41;;                                     //---пин ECHO настраивается на вход
                     pinMode&#40;PinTRIG&#44; OUTPUT&#41;;         
                     digitalWrite&#40;PinTRIG&#44; LOW&#41;; 
                     flag_REQUEST =false;                                         //---сброс флага запуска в обработчике прерываний 
                     StageReset=0;  break;                                         //---сброс номера этапа обработчика
         default: StageReset=0; break;                                          //---на всякй случай.
        }     
 } 
//-----------------------------------------------------------------------------------------------------------      
//   Выход&#44; куда переходят все этапы алгоритма
//-----------------------------------------------------------------------------------------------------------
End1:  Led =Led; 

