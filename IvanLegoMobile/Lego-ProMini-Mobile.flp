NumIRQ = digitalPinToInterrupt&#40;PinECHO&#41;;
//---Уустанавливаем режим работы выводов
pinMode&#40;PinTRIG&#44; OUTPUT&#41;;
pinMode&#40;PinECHO&#44; INPUT&#41;;      
switch&#40;NumIRQ&#41;{
	case 0:	attachInterrupt&#40;NumIRQ&#44; func_IRQ0&#44; CHANGE&#41;; break;  //---
	case 1:	attachInterrupt&#40;NumIRQ&#44; func_IRQ1&#44; CHANGE&#41;; break;
	case 2:	attachInterrupt&#40;NumIRQ&#44; func_IRQ2&#44; CHANGE&#41;; break;
	case 3:	attachInterrupt&#40;NumIRQ&#44; func_IRQ3&#44; CHANGE&#41;; break;
	case 4:	attachInterrupt&#40;NumIRQ&#44; func_IRQ4&#44; CHANGE&#41;; break;
	case 5:	attachInterrupt&#40;NumIRQ&#44; func_IRQ5&#44; CHANGE&#41;; break;
	case 6:	attachInterrupt&#40;NumIRQ&#44; func_IRQ6&#44; CHANGE&#41;; break;
	case 7:	attachInterrupt&#40;NumIRQ&#44; func_IRQ7&#44; CHANGE&#41;; break;
	case 8:	attachInterrupt&#40;NumIRQ&#44; func_IRQ8&#44; CHANGE&#41;; break;
	case 9:	attachInterrupt&#40;NumIRQ&#44; func_IRQ9&#44; CHANGE&#41;; break;  }
//--Устанавливаем уровень логического «0» на выводе TRIG
digitalWrite&#40;PinTRIG&#44; LOW&#41;;
