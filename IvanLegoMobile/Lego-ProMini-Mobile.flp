													<sixx.object sixx.id="1621" sixx.name="string" sixx.type="String" sixx.env="Core" >//---сохраняем номера выводов и определяем номер прерывания. 
//---Сохраняем номера выводов 
      PinTRIG=TRIG;
      PinECHO=ECHO;
//--Определяем номер прерывания. 
//  Если в контроллере нет прерывания соответствующей пину ECHO 
//   &#40;digitalPinToInterrupt&#40;ECHO&#41; возвращает NOT_AN_INTERRUPT &#40;-1&#41;&#44; значит пин ECHO 
//                не обслуживает прерывание и мы обозначим этот слeчай как NumIRQ=99.  
      if &#40;digitalPinToInterrupt&#40;PinECHO&#41;==NOT_AN_INTERRUPT&#41;NumIRQ=99;  //Если функция  
       else NumIRQ = digitalPinToInterrupt&#40;PinECHO&#41;; 
//---Устанавливаем режим работы выводови и настраиваем обработчики прерываний
      if &#40;NumIRQ&#33;=99&#41;{
//------Настраиваем обработчики прерываний                 
        switch&#40;NumIRQ&#41;{
          case 0: attachInterrupt&#40;NumIRQ&#44; HC_func_IRQ0&#44; CHANGE&#41;; break;
          case 1: attachInterrupt&#40;NumIRQ&#44; HC_func_IRQ1&#44; CHANGE&#41;; break;
          case 2: attachInterrupt&#40;NumIRQ&#44; HC_func_IRQ2&#44; CHANGE&#41;; break;
          case 3: attachInterrupt&#40;NumIRQ&#44; HC_func_IRQ3&#44; CHANGE&#41;; break;
          case 4: attachInterrupt&#40;NumIRQ&#44; HC_func_IRQ4&#44; CHANGE&#41;; break;
          case 5: attachInterrupt&#40;NumIRQ&#44; HC_func_IRQ5&#44; CHANGE&#41;; break; 
          case 99: break;}
//------Настраиваем порты
      pinMode&#40;PinTRIG&#44; OUTPUT&#41;;
      pinMode&#40;PinECHO&#44; INPUT&#41;;            
//------Устанавливаем уровень логического «0» на выводе TRIG
      digitalWrite&#40;PinTRIG&#44; LOW&#41;; }</sixx.object>
